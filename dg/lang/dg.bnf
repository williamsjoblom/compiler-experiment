unit = Expr			@CompilationUnit;

Expr = <num>			@Number
     |  :group "(" Expr ")"
     || :right "-" Expr		@NegExpr
     || :left Expr "*" Expr 	@MulExpr
     |  :left Expr "/" Expr    	@DivExpr
     || :left Expr "+" Expr	@AddExpr
     |  :left Expr "-" Expr	@SubExpr
     || :right Expr "=" Expr	@AssExpr;
